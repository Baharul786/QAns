document.querySelectorAll(".chapter-list button").forEach(btn => {
  btn.addEventListener("click", () => {
    const num = btn.getAttribute("data-chapter");
    contentBox.innerHTML = chapters[num] || "<p>Data not found!</p>";
    contentBox.style.display = "block";
    contentBox.scrollIntoView({behavior:"smooth"});
  });
});
